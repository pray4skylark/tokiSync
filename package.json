  "bugs": {
    "url": "https://github.com/pray4skylark/tokiSync/issues"
  },
  "homepage": "https://github.com/pray4skylark/tokiSync#readme",
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.4",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.6.6",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.3",
    "vite": "^7.3.1",
    "vue-loader": "^17.3.1",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.7.15",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "autoprefixer": "^10.4.24",
    "dexie": "^3.2.4",
    "jszip": "^3.10.1",
    "vue": "^3.3.4"
  }
